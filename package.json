    "@webassemblyjs/cli": "^1.11.1",
    "@webassemblyjs/wast-parser": "^1.11.1",
    "assemblyscript": "^0.20.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.3"
  }
}
