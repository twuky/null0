{
  "name": "null0",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start:web": "npx -y live-server --ignore=src targets/web",
    "start:wasm": "npx -y onchange --initial 'sources/*' -- npm run build:debug",
    "start": "run-p start:*",
    "build:debug": "asc sources/actionscript/demo.ts --exportRuntime --target debug",
    "build:release": "asc sources/actionscript/demo.ts --exportRuntime -Ospeed --noAssert --target release",
    "deploy": "npx -y surge targets/web null0.surge.sh",
    "predeploy": "npm run build:debug"
  },
  "keywords": [],
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.20.4",
    "npm-run-all": "^4.1.5"
  }
}
